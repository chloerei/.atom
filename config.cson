"*":
  welcome:
    showOnStartup: false
  core:
    autoHideMenuBar: true
    disabledPackages: [
      "exception-reporting"
      "spell-check"
    ]
  editor:
    invisibles:
      {}
    softWrap: true
    softWrapAtPreferredLineLength: true
    scrollPastEnd: true
    fontFamily: "Droid Sans Mono, Droid Sans Fallback"
  "vim-mode":
    useClipboardAsDefaultRegister: true
